SUBDIR        = source/apputils/templates
SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
OBJECT        = ../../..

default: library

include $(OBJECT)/config/Makefile.config


SUBDIRS = automaticXd special

library:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
lib1d:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
lib2d:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
lib3d:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
libXd:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
clean:
	for DIR in $(SUBDIRS); do (cd $$DIR && $(MAKE) $@) || exit 1; done
