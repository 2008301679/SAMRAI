SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/apputils/templates/special
VPATH	      =	@srcdir@
OBJECT        = ../../../..

default: library

include $(OBJECT)/config/Makefile.config

library:	lib1d lib2d lib3d

OBJS          = pdat__IndexData-NDIM_appu__CutCell_NDIM_$(NDIM).o \
		tbox__Array-pdat__IndexDataNode-NDIM_appu__CutCell_NDIM_$(NDIM).o \
            	tbox__List-pdat__IndexDataNode-NDIM_appu__CutCell_NDIM_$(NDIM).o \
            	tbox__Array-pdat__NodeIndex-NDIM$(NDIM).o
lib1d:
	$(MAKE) NDIM=1 libXd
lib2d:
	$(MAKE) NDIM=2 libXd
lib3d:
	$(MAKE) NDIM=3 libXd
libXd:		$(OBJS)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
