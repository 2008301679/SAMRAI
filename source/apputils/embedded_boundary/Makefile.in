SAMRAI        = @top_srcdir@
SRCDIR        = @srcdir@
SUBDIR        = source/apputils/embedded_boundary
VPATH         = @srcdir@
OBJECT        = ../../..

default: library

include $(OBJECT)/config/Makefile.config

library: lib1d lib2d lib3d

# For classes that are non-dimensional.
OBJS= appu_ebdry2d.o appu_ebdry3d.o

OBJS2D=

OBJS3D=

ifeq ($(NDIM),2)
OBJSXD:=$(OBJS2D)
endif

ifeq ($(NDIM),3)
OBJSXD:=$(OBJS3D)
endif

lib1d:

lib2d:		libXd $(OBJS2D)

lib3d:		libXd $(OBJS3D)

libXd:		$(OBJS) $(OBJSXD)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend

