SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/toolbox/timers
VPATH	      =	@srcdir@
OBJECT        = ../../..

default: library

include $(OBJECT)/config/Makefile.config

library:	lib1d lib2d lib3d

OBJS	      = Clock.o \
		Statistic.o Statistician.o \
		StatTransaction.o	\
		Timer.o TimerManager.o

lib1d:		$(OBJS)
lib2d:		$(OBJS)
lib3d:		$(OBJS)
libXd:		$(OBJS)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
