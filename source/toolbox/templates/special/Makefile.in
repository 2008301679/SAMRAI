SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/toolbox/templates/special
VPATH	      =	@srcdir@
OBJECT        = ../../../..

default: library

include $(OBJECT)/config/Makefile.config

library:	lib1d lib2d lib3d

OBJS	      = ComplexSpecial.o  MathUtilitiesSpecial.o StringSpecial.o  tbox_List-tbox_TimerPointer.o \
		stl-FundamentalTypes.o
lib1d:		$(OBJS)
lib2d:		$(OBJS)
lib3d:		$(OBJS)
libXd:		$(OBJS)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
