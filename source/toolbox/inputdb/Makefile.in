SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/toolbox/inputdb
VPATH	      =	@srcdir@
OBJECT        = ../../..

default: library

include $(OBJECT)/config/Makefile.config

library:	lib1d lib2d lib3d

OBJS	      = Grammar.o InputDatabase.o InputManager.o	\
		Parser.o Scanner.o

lib1d:		$(OBJS)
lib2d:		$(OBJS)
lib3d:		$(OBJS)
libXd:		$(OBJS)

clean:
		$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
