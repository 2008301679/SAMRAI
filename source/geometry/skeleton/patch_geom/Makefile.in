SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/geometry/skeleton/patch_geom
VPATH	      =	@srcdir@
OBJECT        = ../../../..

# For classes that are non-dimensional.
OBJS=

default: library

include $(OBJECT)/config/Makefile.config

library: lib1d lib2d lib3d

# These are for the standard dimensions
# E.G. make lib2d
lib1d:
	$(MAKE) NDIM=1 libXd

lib2d:
	$(MAKE) NDIM=2 libXd

lib3d:
	$(MAKE) NDIM=3 libXd


# This will work for whatever the user sets NDIM to be.
# E.G. make NDIM=5
libXd:	$(OBJS)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
