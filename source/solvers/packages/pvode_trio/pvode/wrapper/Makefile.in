SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/solvers/packages/pvode_trio/pvode/wrapper
VPATH	      =	@srcdir@
OBJECT        = ../../../../../..

default: library

include $(OBJECT)/config/Makefile.config

OBJS          = PVODEAbstractFunctions.o \
                PVODESolver.o

library:	$(OBJS)

lib1d:		$(OBJS)

lib2d:		$(OBJS)

lib3d:		$(OBJS)

libXd:		$(OBJS)


clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
