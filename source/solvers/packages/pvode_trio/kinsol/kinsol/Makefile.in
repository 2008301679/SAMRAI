SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/solvers/packages/pvode_trio/kinsol/kinsol
VPATH	      =	.
OBJECT        = ../../../../../..

default: library

include $(OBJECT)/config/Makefile.config

library:	kinsol_test
lib1d:		kinsol_test
lib2d:		kinsol_test
lib3d:		kinsol_test
libXd:		kinsol_test

kinsol_test:
		if [ -r kinsol.c ]; then $(MAKE) kinsol_libs; fi

OBJS	      = iterativ.o kinsol.o kinspgmr.o llnlmath.o spgmr.o

kinsol_libs:	$(OBJS)

clean:
	$(SAMCLEAN)

include @srcdir@/Makefile.depend
