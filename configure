# By default make the object files depend on the template implementation files
# Turn this off for known cases that do not require it.

# Check whether --enable-implicit-template-instantiation or --disable-implicit-template-instantiation was given.
if test "${enable_implicit_template_instantiation+set}" = set; then
  enableval="$enable_implicit_template_instantiation"

else
  enable_implicit_template_instantiation=no

fi;

if test "${enable_implicit_template_instantiation}" = 'no'; then
case "$CXX_ID" in
   gnu)
      CXX_FLAGS="-fno-implicit-templates $CXX_FLAGS"
      ;;
   ibm)
      CXX_FLAGS="-qnotempinc $CXX_FLAGS"
      ;;
   intel)
      :
      ;;
   sunpro)
      CXX_FLAGS="-instances=explicit $CXX_FLAGS"
      ;;
esac
fi

DEPENDS_ON_TEMPLATE_IMPLEMENTATION="yes"
case $CXX_ID in
   gnu)
      DEPENDS_ON_TEMPLATE_IMPLEMENTATION="no"
      CXX_FLAGS="-Wall $CXX_FLAGS"
      case $ARCH in
         IRIX64)
            CXX_FLAGS="-mabi=64 $CXX_FLAGS"
            ;;
	 solaris)
	    ;;
	 CYGWIN)
	    CXX_FLAGS="$CXX_FLAGS"
	    ;;
      esac
      ;;
   kai)
      #  compiler may require template includes, we don't test anymore
      cat >>confdefs.h <<\_ACEOF
#define INCLUDE_TEMPLATE_IMPLEMENTATION 1
_ACEOF

      case $ARCH in
         IRIX64)
            CXX_FLAGS="-64 $CXX_FLAGS"
            ;;
      esac
      CXX_FLAGS="--keep_gen_c $CXX_FLAGS"
      ;;
   sunpro)
      DEPENDS_ON_TEMPLATE_IMPLEMENTATION="no"
      CXX_FLAGS="-qrtti=all $CXX_FLAGS"
      case $ARCH in
         solaris | sun4)
            CXX_FLAGS="-instances=explicit $CXX_FLAGS"
            CXX_FLAGS="+p $CXX_FLAGS"
            ;;
      esac
      ;;
   xlc)
      DEPENDS_ON_TEMPLATE_IMPLEMENTATION="no"
      CXX_FLAGS="-qrtti=all $CXX_FLAGS"
      ;;
   intel)
      DEPENDS_ON_TEMPLATE_IMPLEMENTATION="no"
      #  compiler requires template includes
      cat >>confdefs.h <<\_ACEOF
#define INCLUDE_TEMPLATE_IMPLEMENTATION 1
_ACEOF

      case $ARCH in
         LINUX)
            CXX_FLAGS="$CXX_FLAGS -wd1476 -wd1505"
            ;;
      esac
      ;;
    insure)
      case $ARCH in
         solaris | sun4)
            CXX_FLAGS="+p $CXX_FLAGS"
            LIB_NAME="$LIB_NAME -Zsl"
            ;;
      esac
      ;;
    *)
      # By default include the template impls during compile
      cat >>confdefs.h <<\_ACEOF
#define INCLUDE_TEMPLATE_IMPLEMENTATION 1
_ACEOF

esac



